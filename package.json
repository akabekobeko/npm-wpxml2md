{
  "name": "wpxml2md",
  "description": "Convert the WordPress XML file to Markdown files.",
  "version": "1.0.3",
  "author": "akabeko (http://akabeko.me/)",
  "license": "MIT",
  "homepage": "https://github.com/akabekobeko/npm-wpxml2md#readme",
  "engines": {
    "node": ">= 6"
  },
  "main": "index.js",
  "bin": "bin/main.js",
  "files": [
    "bin",
    "lib",
    "index.js"
  ],
  "keywords": [
    "Convert",
    "WordPress",
    "XML",
    "Markdown"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/akabekobeko/npm-wpxml2md.git"
  },
  "bugs": {
    "url": "https://github.com/akabekobeko/npm-wpxml2md/issues"
  },
  "esdoc": {
    "source": "./src",
    "destination": "./esdoc",
    "test": {
      "type": "mocha",
      "source": "./test"
    }
  },
  "babel": {
    "presets": [
      "latest"
    ],
    "env": {
      "development": {
        "presets": [
          "power-assert"
        ]
      }
    }
  },
  "scripts": {
    "test": "mocha --timeout 50000 --compilers js:babel-register test/**/*.test.js",
    "start": "npm run watch",
    "esdoc": "esdoc",
    "build": "babel src --out-dir ./",
    "watch": "babel src --out-dir ./ --watch",
    "prepublish": "npm run build"
  },
  "dependencies": {
    "collapse-whitespace": "^1.1.4",
    "jsdom": "^9.8.3",
    "xml2js": "^0.4.17"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-power-assert": "^1.0.0",
    "babel-register": "^6.18.0",
    "esdoc": "^0.4.8",
    "mocha": "^3.2.0",
    "power-assert": "^1.4.2"
  }
}
