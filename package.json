{
  "name": "wpxml2md",
  "description": "Convert the WordPress XML file to Markdown files.",
  "version": "1.1.1",
  "author": "akabeko (http://akabeko.me/)",
  "license": "MIT",
  "homepage": "https://github.com/akabekobeko/npm-wpxml2md#readme",
  "engines": {
    "node": ">= 6"
  },
  "main": "index.js",
  "bin": "src/bin/main.js",
  "files": [
    "src/bin",
    "src/lib",
    "index.js"
  ],
  "keywords": [
    "Convert",
    "WordPress",
    "XML",
    "Markdown"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/akabekobeko/npm-wpxml2md.git"
  },
  "bugs": {
    "url": "https://github.com/akabekobeko/npm-wpxml2md/issues"
  },
  "esdoc": {
    "source": "./src",
    "destination": "./esdoc",
    "test": {
      "type": "mocha",
      "source": "./test"
    },
    "plugins": [
      {
        "name": "esdoc-node"
      }
    ]
  },
  "babel": {
    "env": {
      "development": {
        "presets": [
          "power-assert"
        ]
      }
    }
  },
  "scripts": {
    "test": "mocha --timeout 50000 --compilers js:babel-register test/**/*.test.js",
    "esdoc": "esdoc"
  },
  "dependencies": {
    "collapse-whitespace": "^1.1.4",
    "jsdom": "^9.9.1",
    "xml2js": "^0.4.17"
  },
  "devDependencies": {
    "babel-preset-power-assert": "^1.0.0",
    "babel-register": "^6.22.0",
    "esdoc": "^0.5.2",
    "esdoc-node": "^1.0.0",
    "mocha": "^3.2.0",
    "power-assert": "^1.4.2"
  }
}
